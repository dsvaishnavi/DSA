class List:
    def __init__(self,val=0,next=None):
        self.val=val
        self.next=next
node1=List(10)
node2=List(20)
node3=List(30)
node4=List(40)

#linking list

node1.next=node2
node2.next=node3
node3.next=node4

def PrintList(head):
    current=head
    while current:
        print(current.val,end="---->")
        current=current.next
    print("no")
PrintList(node1)
    
    
